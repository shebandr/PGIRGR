<Window x:Class="PGIRGR.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PGIRGR"
        mc:Ignorable="d"
        Title="MainWindow" Height="800" Width="1400">
    
    <StackPanel Orientation="Horizontal">
        <Button Height="20" VerticalAlignment="Top" Margin="5" x:Name="mainButton" Click="mainButton_Click">открыть файл</Button>
        <Image Width="600" Height="450" x:Name="inputImage" Margin="5"/>
        <Image Width="600" Height="450" x:Name="outputImage" Margin="5"/>


    </StackPanel>
    
</Window>
